<%- include('extra/_header') %>
<main id="site-main">
  <div class="container">
    <div class="box-nav d-flex justify-between">
      <div class="filter">
        <a href="/" class="fas fa-angle-double-left">All Projects</a>
      </div>
    </div>
    <div class="form-title text-center">
      <h2>Update Project</h2>
      <span>Use below form to update Project</span>
    </div>
    <!-- form handling -->
    <form method="POST" id="update-user">
        <div class="new-issue">
          <div class="form-group">
            <label for="name">Project-Name</label>
            <input
              type="text"
              name="project-name"
              value=""
              placeholder="add name"
            />
          </div>
          <div class="form-group">
            <label for="Description">Description</label>
            <input
              type="text"
              name="Description"
              value=""
              placeholder="add author"
            />
          </div>
          <div class="form-group">
            <label for="issue">Author</label>
            <input
              type="text"
              name="author"
              value=""
              placeholder="add author"
            />
          </div>
         
          </div>
          <div class="form-group">
              <button class="btn" type="submit">Save</button>
            </div>
        </div>
      </form>
  </div>
</main>
<%- include('extra/_footer') %>
